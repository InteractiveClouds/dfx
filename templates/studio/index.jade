doctype html
html
    head
        meta(http-equiv="content-type", content="text/HTML;charset=UTF-8")
        meta(name="viewport", content="width=device-width,initial-scale=1")
        title DreamFace Studio
        link(rel="stylesheet", href="/css/dfx/dfx-bootstrap.min.css", media="screen")
        link(rel="stylesheet", href="/css/vendor/font-awesome.min.css", media="screen")
        link(rel="stylesheet", href="/css/vendor/jquery-ui.min.css", media="screen")
        link(rel="stylesheet", href="/css/vendor/jqtree.min.css", media="screen")
        link(rel="stylesheet", href="/css/dfx/dfx.min.css", media="screen")
        link(rel="stylesheet", href="/css/dfx/dfx-dropzone.min.css", media="screen")
        link(rel="stylesheet", href="/css/dfx/dfx-tree.min.css", media="screen")
        link(rel="stylesheet", href="/css/dfx/dfx-flexigrid.min.css", media="screen")
        link(rel="stylesheet", href="/css/dfx/block_s-select.min.css", media="screen")

        link(rel="stylesheet", href="/node_modules/codemirror/lib/codemirror.css", media="screen")
        link(rel="stylesheet", href="/node_modules/codemirror/theme/abcdef.css", media="screen")
        link(rel="stylesheet", href="/node_modules/codemirror/addon/dialog/dialog.css", media="screen")
